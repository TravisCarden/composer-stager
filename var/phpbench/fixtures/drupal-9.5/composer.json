        "drupal/search404": "2.1.2",
        "drupal/search_api": "1.29.0",
        "drupal/search_api_autocomplete": "1.7.0",
        "drupal/search_api_solr": "4.2.12",
        "drupal/security_review": "2.0.1",
        "drupal/select2": "1.15.0",
        "drupal/sharethis": "3.0.2",
        "drupal/shield": "1.7.0",
        "drupal/simple_sitemap": "4.1.6",
